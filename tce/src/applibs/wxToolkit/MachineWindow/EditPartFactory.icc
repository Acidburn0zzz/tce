/**
 * Registers a new factory to which the Factory can delegate creation
 * of EditParts it cannot create itself.
 *
 * @param factory New Factory to be registered.
 */
inline void
EditPartFactory::registerFactory(Factory* factory) {
    factories_.push_back(factory);
}
