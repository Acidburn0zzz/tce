/**
 * Casts a given type to integer.
 *
 * @param src Source type to be casted.
 * @param index Memory index.
 */
template<typename source>
int
TargetMemory::asInteger(source& src, int index) {
    return (reinterpret_cast<int*>(&src))[index];
}
