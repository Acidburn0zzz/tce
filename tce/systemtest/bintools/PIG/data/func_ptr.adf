    </port>
    <operation>
      <name>stw</name>
      <bind name="1">P1</bind>
      <bind name="2">P2</bind>
      <pipeline>
        <resource name="res0">
          <start-cycle>1</start-cycle>
          <cycles>1</cycles>
        </resource>
        <resource name="res1">
          <start-cycle>2</start-cycle>
          <cycles>1</cycles>
        </resource>
        <reads name="1">
          <start-cycle>0</start-cycle>
          <cycles>1</cycles>
        </reads>
        <reads name="2">
          <start-cycle>0</start-cycle>
          <cycles>1</cycles>
        </reads>
