        <bool>
          <name>boolean</name>
          <index>2</index>
        </bool>
      </inverted-expr>
    </guard>
    <guard>
      <inverted-expr>
        <bool>
          <name>boolean</name>
          <index>3</index>
        </bool>
      </inverted-expr>
    </guard>
    <guard>
      <inverted-expr>
        <bool>
          <name>boolean</name>
          <index>4</index>
        </bool>
      </inverted-expr>
