    <guard>
      <simple-expr>
        <bool>
          <name>boolean</name>
          <index>0</index>
        </bool>
      </simple-expr>
    </guard>
    <guard>
      <simple-expr>
        <bool>
          <name>boolean</name>
          <index>1</index>
