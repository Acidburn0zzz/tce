  <global-control-unit name="control_unit">
    <port name="trigger">
      <connects-to>S1_1_4</connects-to>
      <width>32</width>
      <triggers/>
      <sets-opcode/>
    </port>
    <special-port name="RA">
      <connects-to>S2_3</connects-to>
      <connects-to>S1_1_4_1</connects-to>
      <width>32</width>
    </special-port>
    <return-address>RA</return-address>
    <ctrl-operation>
      <name>jump</name>
      <bind name="1">trigger</bind>
      <pipeline>
        <reads name="1">
          <start-cycle>0</start-cycle>
          <cycles>1</cycles>
        </reads>
      </pipeline>
    </ctrl-operation>
    <ctrl-operation>
      <name>call</name>
      <bind name="1">trigger</bind>
      <pipeline>
        <reads name="1">
          <start-cycle>0</start-cycle>
          <cycles>1</cycles>
        </reads>
      </pipeline>
    </ctrl-operation>
    <address-space>instructions</address-space>
    <delay-slots>3</delay-slots>
    <guard-latency>1</guard-latency>
  </global-control-unit>

</adf>
