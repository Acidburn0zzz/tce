  <register-file name="RF">
    <type>normal</type>
    <size>16</size>
    <width>32</width>
    <max-reads>1</max-reads>
    <max-writes>1</max-writes>
    <guard-latency>1</guard-latency>
    <port name="r0">
      <connects-to>RF_o1</connects-to>
    </port>
    <port name="w0">
      <connects-to>RF_i1</connects-to>
    </port>
  </register-file>

  <register-file name="boolRF">
    <type>normal</type>
    <size>1</size>
    <width>1</width>
    <max-reads>1</max-reads>
    <max-writes>1</max-writes>
    <port name="P1">
      <connects-to>boolRF_i1</connects-to>
    </port>
    <port name="P2">
      <connects-to>boolRF_o1</connects-to>
    </port>
