    </short-immediate>
  </bus>

  <socket name="RF_i1">
    <reads-from>
      <bus>B1</bus>
      <segment>seg1</segment>
    </reads-from>
    <reads-from>
      <bus>B2</bus>
      <segment>seg1</segment>
    </reads-from>
  </socket>

  <socket name="GCU_t1">
    <reads-from>
      <bus>B1</bus>
      <segment>seg1</segment>
